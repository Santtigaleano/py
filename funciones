'''

ganador =3
def jugar():
    #rango_valores =[1,2,3,4,5]
    contador=1
    while contador<4:
        sel=int(input("ingrese un valor del 1 al 5"))
        if sel != ganador:
            contador+1
        if contador ==4:
            print("game over")
            break
            
        else:
            print("GANASTE")
            break

jugar()


while True:
    jugar()
    seguir_jugando=input("quiere seguir jugando (s/)")
    if seguir_jugando=="s":
        jugar()
    else:
        print("Chau, buen día")
        break
