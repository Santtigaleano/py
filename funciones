#Hallar radio esfera

def volumen_esfera(radio):
    PI=3.14
    volumen=4/3*PI*radio**3
    print("El volumen es : ", volumen)

ingrese_radio= float(input("Ingrese el numero: "))
volumen_esfera(ingrese_radio)
