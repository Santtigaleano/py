#RETORNAR MULTIPLES VALORES EN UNA FUNCION

'''
ejemplo mal
def datos ():
    temp=float(input("ingrese una temperatura: "))
    humedad=float(input("Ingrese la humedad: "))
    return temp,humedad
datos()
'''


def datos2():
    valores=[]
    temp=float(input("Ingrese una temperatura: "))
    hume=float(input("Ingrese la humedad: "))
    valores.append(temp)
    valores.append(hume)
    return valores

datos2()
print (datos2())

#valor fuera de funcion y valor dentro